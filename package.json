{
  "name": "gatsby-orga",
  "description": "Gatsby starter website with org-mode",
  "version": "1.0.0",
  "author": "Xiaoxing Hu <dawnstar.hu@gmail.com>",
  "dependencies": {
    "gatsby": "^2.0.47",
    "gatsby-plugin-typography": "^2.2.1",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-orga": "^1.0.1",
    "mime": "^2.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17",
    "typography-plugin-code": "^0.16.11",
    "typography-theme-github": "^0.16.18"
  },
  "keywords": [
    "gatsby",
    "org-mode",
    "orga"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build --prefix-paths",
